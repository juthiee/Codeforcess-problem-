#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin >> n;
        string s;
        cin >> s;

        vector<int> m;

        for (int i = 0; i < n; i++)
        {
            if (s[i] == '0')
            {
                m.push_back(i + 1);
            }
        }

        cout << m.size() << "\n";

        for (int i = 0; i < m.size(); i++)
        {
            cout << m[i] << " ";
        }

        cout << "\n";
    }

    return 0;
}

